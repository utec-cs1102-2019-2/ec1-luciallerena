#include <iostream>
#include <ctime>
#include <cstdlib>
using namespace std;


int main() {
    int filas = 8;
    int columnas = 4;
    int filas2 = 8;
    int columnas2 = 4;
    int col = 4;
    int fil = 8;

    int **matrix = nullptr;
    matrix = new int *[filas];
    for (int i = 0; i < filas; i++) {
        matrix[i] = new int[columnas];
        for (int j = 0; j < columnas; j++) {
            matrix[i][j] = rand() % 10;
            cout << matrix[i][j] << "\t";
        }
        cout << endl;
    }
    cout << endl;

    int **matriz = nullptr;
    matriz = new int *[filas2];
    for (int n = 0; n < filas2; n++) {
        matriz[n] = new int[columnas2];
        for (int m = 0; m < columnas2; m++) {
            matriz[n][m] = rand() % 10;
            cout << matriz[n][m] << "\t";
        }
        cout << endl;
    }
    


    int **matris = nullptr;
    matris = new int *[fil];
    for (int x = 0; x < fil; x++) {
        matris[x] = new int[col];
        for (int y = 0; y < col; y++) {

            cout << endl;
        }


        return 0;
    }

}
